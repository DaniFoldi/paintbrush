{
  "name": "paintbrush-ui",
  "version": "0.0.1",
  "description": "A flat UI component library",
  "scripts": {
    "build:docs": "ts-node-esm scripts/docs.ts",
    "build:nuxt": "nuxi build",
    "build": "npm-run-all build:docs build:nuxt",
    "clean": "rimraf coverage .nuxt .output",
    "dev": "nuxi dev",
    "license": "npm-run-all license:*",
    "license:prod": "license-checker --summary --production --unknown --onlyAllow \"0BSD;Apache-2.0;BSD-2-Clause;BSD-3-Clause;ISC;MIT\"",
    "license:all": "license-checker --summary --onlyAllow \"0BSD;Apache*;Apache-2.0;BSD-2-Clause;BSD-3-Clause;CC0-1.0;CC-BY-3.0;CC-BY-4.0;CC-BY-ND-4.0;ISC;MIT;Python-2.0\"",
    "lint": "npm-run-all lint:*",
    "lint:css": "stylelint --cache --cache-location .stylelintcache --report-descriptionless-disables --report-needless-disables --report-invalid-scope-disables \"**/*.{scss,vue}\" \"*.{scss,vue}\"",
    "lintw:css": "chokidar \"**/*.{scss,vue}\" -i \"{coverage,node_modules,.nuxt,.output}/**\" -c \"npm run lint:css -- --fix\"",
    "lint:js": "eslint --ext .cjs,.js,.json,.ts,.vue --cache --cache-file .eslintcache --report-unused-disable-directives .",
    "lintw:js": "chokidar \"**/*.{cjs,js,json,ts,vue}\" -i \"{coverage,node_modules,.nuxt,.output}/**\" -c \"npm run lint:js -- --fix\"",
    "lintw": "npm-run-all --parallel lintw:*",
    "prepare": "nuxi prepare",
    "start": "node .output/server/index.mjs",
    "test": "vitest --coverage --run",
    "typecheck": "nuxi typecheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaniFoldi/paintbrush.git"
  },
  "keywords": [
    "component",
    "component library",
    "components",
    "design",
    "frontend",
    "nuxt",
    "nuxt 3",
    "paintbrush",
    "ui",
    "vue",
    "web"
  ],
  "type": "module",
  "author": "DaniFoldi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DaniFoldi/paintbrush/issues"
  },
  "homepage": "https://github.com/DaniFoldi/paintbrush#readme",
  "devDependencies": {
    "@danifoldi/eslint-plugin-eslint-presets": "^1.0.0",
    "@danifoldi/stylelint-presets": "^1.0.0",
    "@nuxt/test-utils-edge": "^3.0.0-27497942.c519cee",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "c8": "^7.11.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.11.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-rulesdir": "^0.2.1",
    "eslint-plugin-unicorn": "^42.0.0",
    "eslint-plugin-vitest": "^0.0.8",
    "eslint-plugin-vue": "^8.4.1",
    "globby": "^13.1.1",
    "license-checker": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "nuxt3": "latest",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "postcss-scss": "^4.0.3",
    "rimraf": "^3.0.2",
    "sass": "^1.49.10",
    "stylelint": "^14.6.1",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-high-performance-animation": "^1.6.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5",
    "vitest": "^0.9.2",
    "vue-tsc": "^0.34.6"
  },
  "dependencies": {
    "@fontsource/inter": "^4.5.5",
    "@fontsource/playfair-display": "^4.5.6",
    "@fontsource/raleway": "^4.5.3",
    "highlight.js": "^11.5.0",
    "phosphor-icons": "^1.4.2"
  }
}
