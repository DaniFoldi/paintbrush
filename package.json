{
  "name": "paintbrush-ui",
  "version": "0.0.2",
  "description": "A flat UI component library",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "node .output/server/index.mjs",
    "lint:watch": "concurrently -t 1000 \"npm run lint:js:watch\" \"npm run lint:css:watch\"",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js:watch": "chokidar \"**/*.{js,ts,vue}\" -i \"{node_modules,.nuxt,.output}/**\" -c \"npm run lint:js -- --fix\"",
    "lint:css:watch": "chokidar \"**/*.{sass,vue}\" -i \"{node_modules,.nuxt,.output}/**\" -c \"npm run lint:css -- --fix\"",
    "lint:js": "npx eslint --ext .js,.ts,.vue .",
    "lint:css": "npx stylelint \"**/*.{sass,vue}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaniFoldi/paintbrush.git"
  },
  "keywords": [
    "component",
    "components",
    "design",
    "web",
    "paintbrush",
    "frontend",
    "ui",
    "component library",
    "vue"
  ],
  "author": "DaniFoldi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DaniFoldi/paintbrush/issues"
  },
  "homepage": "https://github.com/DaniFoldi/paintbrush#readme",
  "devDependencies": {
    "@fontsource/raleway": "^4.5.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.8.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.4.0",
    "globby": "^13.1.0",
    "nuxt3": "latest",
    "phosphor-icons": "^1.4.2",
    "postcss-html": "^1.3.0",
    "sass": "^1.49.0",
    "stylelint": "^14.3.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-scss": "^4.1.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
  }
}
