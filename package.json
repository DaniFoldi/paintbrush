{
  "name": "paintbrush-ui",
  "version": "0.4.1",
  "description": "A flat UI component library",
  "scripts": {
    "build:docs": "ts-node-esm scripts/docs.ts",
    "build:nuxt": "nuxi build",
    "build": "npm-run-all build:docs build:nuxt",
    "buildw:docs": "chokidar \"components/*.vue\" \"components/**/*.vue\" -c \"npm run build:docs {path}\"",
    "clean": "rimraf coverage .nuxt .output",
    "dev": "nuxi dev",
    "license": "npm-run-all license:*",
    "license:prod": "license-checker --summary --production --unknown --onlyAllow \"0BSD;Apache-2.0;BSD-2-Clause;BSD-3-Clause;ISC;MIT;CC-BY-4.0\"",
    "license:all": "license-checker --summary --onlyAllow \"0BSD;Apache*;Apache-2.0;BSD-2-Clause;BSD-3-Clause;CC0-1.0;CC-BY-3.0;CC-BY-4.0;CC-BY-ND-4.0;ISC;MIT;Python-2.0\"",
    "lint": "npm-run-all lint:*",
    "lint:css": "stylelint --cache --cache-location .stylelintcache --report-descriptionless-disables --report-needless-disables --report-invalid-scope-disables \"**/*.{scss,vue}\" \"*.{scss,vue}\"",
    "lintw:css": "chokidar \"**/*.{scss,vue}\" -i \"{coverage,node_modules,.nuxt,.output}/**\" -c \"npm run lint:css -- --fix\"",
    "lint:js": "eslint --ext .cjs,.js,.json,.mjs,.ts,.vue --cache --cache-file .eslintcache --report-unused-disable-directives .",
    "lintw:js": "chokidar \"**/*.{cjs,js,json,ts,vue}\" -i \"{coverage,node_modules,.nuxt,.output}/**\" -c \"npm run lint:js -- --fix\"",
    "lintw": "npm-run-all --parallel lintw:*",
    "prepare": "nuxi prepare",
    "start": "node .output/server/index.mjs",
    "test": "vitest --coverage --run",
    "typecheck": "nuxi typecheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaniFoldi/paintbrush.git"
  },
  "keywords": [
    "component",
    "component library",
    "components",
    "design",
    "frontend",
    "nuxt",
    "nuxt 3",
    "paintbrush",
    "ui",
    "vue",
    "web"
  ],
  "type": "module",
  "author": "DaniFoldi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DaniFoldi/paintbrush/issues"
  },
  "homepage": "https://github.com/DaniFoldi/paintbrush#readme",
  "devDependencies": {
    "@danifoldi/eslint-plugin-eslint-presets": "^1.6.0",
    "@danifoldi/stylelint-presets": "^1.2.1",
    "@fontsource/source-code-pro": "^4.5.11",
    "@nuxt/content": "npm:@nuxt/content-edge@^2.1.0-27680354.5a64f46",
    "@nuxt/test-utils": "^3.0.0-rc.6",
    "@nuxtjs/i18n-edge": "8.0.0-alpha.0-27679707.0923cac",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitest/coverage-c8": "^0.22.0",
    "@vueuse/components": "^9.1.0",
    "@vueuse/core": "^9.0.0",
    "@vueuse/nuxt": "^9.1.0",
    "chokidar-cli": "^3.0.0",
    "defu": "^6.1.0",
    "eslint": "^8.22.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-plugin-vitest": "^0.0.8",
    "eslint-plugin-vue": "^9.3.0",
    "globby": "^13.1.2",
    "json5": "^2.2.1",
    "license-checker": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.0.0-rc.6",
    "nuxt-lodash": "^2.2.0",
    "playwright": "^1.25.0",
    "postcss": "^8.4.16",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.4",
    "rimraf": "^3.0.2",
    "sass": "^1.54.4",
    "stylelint": "^14.10.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-high-performance-animation": "^1.6.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-dedent": "^2.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vitest": "^0.22.0",
    "vue-tsc": "^0.40.1"
  },
  "dependencies": {
    "@fontsource/gantari": "^4.5.0",
    "@fontsource/inter": "^4.5.12",
    "@fontsource/manrope": "^4.5.11",
    "@fontsource/playfair-display": "^4.5.11",
    "@fontsource/raleway": "^4.5.10",
    "@formkit/auto-animate": "^1.0.0-beta.1",
    "@pinia/nuxt": "^0.4.0",
    "atropos": "^1.0.2",
    "chart.js": "^3.9.1",
    "codemirror": "^6.0.1",
    "highlight.js": "^11.6.0",
    "parse5": "^6.0.1",
    "phosphor-icons": "^1.4.2",
    "pinia": "^2.0.18",
    "pinia-plugin-persistedstate": "^2.1.1",
    "vue": "^3.2.37",
    "vue-chartjs": "^4.1.1",
    "vue-codemirror": "^6.0.2"
  },
  "overrides": {
    "git-url-parse": "12.0.0"
  }
}
